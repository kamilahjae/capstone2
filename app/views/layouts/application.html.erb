<!DOCTYPE html>
<html>
<head>
  <title>Kamilah's Capstone</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <script src="http://d3js.org/d3.v3.min.js"></script>
  <script src="http://d3js.org/topojson.v1.min.js"></script>
  <!-- <script src="assets/bower_components/d3-tip/index.js"></script> -->
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>

  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
</head>
<body>

<nav class="navWrapper">
  <nav class="navbar navbar-default">
    <p class="navTitle navbar-text">Kamilah's Capstone</p>
    <div class="navTextWrapper">
      <a href="/" class="navbar-text">About</a>
      <a href="/" class="navbar-text ">Contact</a>
    </div><!-- /navTextWrapper -->
  </nav><!-- /navbar -->
</nav><!-- /navWrapper -->

<div class="container-fluid noPadding">

<%= yield %>
  <footer class="footer">
    <div class="btnWrapper">
      <div class="row">
        <button href="/"  class="btn btn-xlarge col-md-4 " /><i class="fa fa-clock-o fa-5x" >Map</i></button>
        <button href="/events/show"  class="btn btn-xlarge col-md-4 " /><i class="fa fa-chevron-up fa-5x" >Chart</i></button>
        <button href="/"  class="btn btn-xlarge col-md-4 " /><i class="fa fa-chevron-up fa-5x" >Worst States</i></button>
      </div><!-- /row -->
    </div><!-- /btnWrapper -->
  </footer>
</div><!-- /container-fluid -->
</body>
</html>
